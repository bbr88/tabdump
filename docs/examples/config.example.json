{
  "vaultInbox": "/Users/<your-user>/obsidian/Inbox/",
  "outputFilenameTemplate": "TabDump {ts}.md",
  "browsers": [
    "Chrome",
    "Safari"
  ],
  "allowlistUrlContains": [
    "mail.google.com",
    "calendar.google.com",
    "slack.com",
    "notion.so"
  ],
  "keepPinnedTabs": true,
  "skipUrlPrefixes": [
    "chrome://",
    "chrome-extension://",
    "about:",
    "file://",
    "safari-web-extension://",
    "favorites://",
    "safari://"
  ],
  "skipTitlesExact": [
    "New Tab",
    "Start Page"
  ],
  "outputGroupByWindow": true,
  "outputIncludeMetadata": false,
  "dryRun": true,
  "dryRunPolicy": "auto",
  "onboardingStartedAt": 0,
  "maxTabs": 30,
  "checkEveryMinutes": 60,
  "cooldownMinutes": 1440,
  "llmEnabled": false,
  "tagModel": "gpt-4.1-mini",
  "llmRedact": true,
  "llmRedactQuery": true,
  "llmTitleMax": 200,
  "maxItems": 0
}
